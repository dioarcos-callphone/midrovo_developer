<odoo>
    <data>
        <record id="access_res_partner_no_update_delete" model="ir.model.access">
            <field name="name">No Update/Delete Partner Access</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="group_id" ref="custom_security_rules.group_no_update_delete"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="0"/>
            <field name="perm_create" eval="0"/>
            <field name="perm_unlink" eval="0"/>
        </record>

        <record id="access_account_move_no_update_delete" model="ir.model.access">
            <field name="name">No Update/Delete Account Move Access</field>
            <field name="model_id" ref="account.model_account_move"/>
            <field name="group_id" ref="custom_security_rules.group_no_update_delete"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_write" eval="0"/>
            <field name="perm_create" eval="0"/>
            <field name="perm_unlink" eval="0"/>
        </record>

        <!-- Repite esto para otros modelos que desees restringir -->
    </data>
</odoo>
