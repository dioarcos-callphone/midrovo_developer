<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <template id="report_product_template_catalog">
        <t t-foreach="docs" t-as="product_template">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                    <div class="page">
                        <h1 t-esc='product_template.name'></h1>
                        <t t-foreach="env['product.template.catalog']._get_report_values(product_template.id)" t-as="result">
                            <span t-esc="result['name']"></span>
                            <!-- <span t-esc="result['imagen']"></span> -->
                        </t>
                        <!-- <t t-foreach="product_template.product_variant_group()" t-as="data">
                            <h3 t-esc="data['name']"></h3>
                            <t t-foreach="data['variants']" t-as="v">
                                <h6 t-esc="v"></h6>
                            </t>
                        </t> -->
                    </div>
                </t>
            </t>
        </t>
    </template>
</odoo>
