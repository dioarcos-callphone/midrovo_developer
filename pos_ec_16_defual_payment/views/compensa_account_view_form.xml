<odoo>
    <record id="compensa_account_form_view" model="ir.ui.view">
        <field name="name">compensa.account.view.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form" />
        <field name="priority">20</field>
        <field name="arch" type="xml">
           <xpath expr="//field[@name='l10n_ec_sri_payment_id']" position="after">
                    <field
                        name="compensa"
                        string="Compensacion de Deuda"
                        placeholder="Compensacion"
                    />
            </xpath>
        </field>
    </record>
</odoo>
