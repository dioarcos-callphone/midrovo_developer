<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
    
        <record id="sri_printer_point_inherit_form" model="ir.ui.view">
            <field name="name">sri.printer.point.inherit.form</field>
            <field name="model">sale.shop</field>
            <field name="inherit_id" ref="ec_sri_authorizathions.sale_shop_multi_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='project_id']" position="replace">
                    <field name="project_id" invisible="1"/>
                </xpath>
                <xpath expr="//field[@name='printer_point_ids']/tree/field[@name='number']" position="after">
                    <field name="analytic_id" />
                </xpath>
                <xpath expr="//field[@name='printer_point_ids']/form/group/field[@name='number']" position="after">
                    <field name="analytic_id" />
                </xpath>
            </field>
        </record>
    
    </data>
    
</odoo>