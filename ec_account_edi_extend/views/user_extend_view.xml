<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>

        <record id="res_user_multi_form_extend_view" model="ir.ui.view">
            <field name="name">res.user.multi.form.extend.view</field>
            <field name="model">res.users</field>
            <field name="type">form</field>
            <field name="inherit_id" ref="ec_sri_authorizathions.res_user_multi_form_view" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='printer_default_id']" position="replace">
                    <field
                        name="printer_default_ids"
                        colspan="4"
                        widget="many2many_tags"
                        options="{'no_create': True. 'color_field': '1'}"                      
                        />
                </xpath>
            </field>
        </record>
    
    </data>
    
</odoo>