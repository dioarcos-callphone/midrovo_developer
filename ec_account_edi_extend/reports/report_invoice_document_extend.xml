<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <template id="report_invoice_document_extend" name="Invoice Document" inherit_id="ec_account_edi.report_invoice_document">
            <xpath expr="//t[@t-if=&quot;document.move_type == 'liquidacion'&quot;]" position="replace">
                <t t-if="document.liquidation == True"
                   t-set="document_string" 
                   t-value="'LIQUIDACION DE COMPRA'" />
            </xpath>
        </template>
    </data>
</odoo>
